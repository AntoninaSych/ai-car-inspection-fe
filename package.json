{
  "name": "ai-car",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "author": "Antonina Sych <antoninasych@gmail.com>",
  "contributors": [
    "Roman Onishchenko <romanindev@gmail.com>"
  ],
  "license": "UNLICENSED",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "audit": "npm audit --production --audit-level=critical",
    "lint": "npm-run-all --parallel lint:*",
    "lint:js": "eslint src/**/*.{js,jsx}",
    "preview": "vite preview",
    "precommit": "pretty-quick --staged",
    "format": "prettier --log-level warn --write \"src/**/*.{js,jsx}\"",
    "lint-staged": "lint-staged",
    "postinstall": "husky"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@hookform/resolvers": "5.0.1",
    "@mui/icons-material": "^7.3.5",
    "@mui/material": "^7.0.2",
    "@mui/utils": "^7.3.5",
    "@reduxjs/toolkit": "2.2.7",
    "@tanstack/react-query": "^5.90.11",
    "axios": "1.7.7",
    "clsx": "2.1.1",
    "i18next": "^25.6.3",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "7.56.2",
    "react-hot-toast": "2.4.1",
    "react-i18next": "^16.3.5",
    "react-icons": "5.5.0",
    "react-loader-spinner": "6.1.6",
    "react-redux": "9.1.2",
    "react-router-dom": "6.30.0",
    "react-select": "5.10.1",
    "redux-persist": "6.0.0",
    "yup": "1.6.1"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.0",
    "@commitlint/config-conventional": "19.8.0",
    "@eslint/js": "9.9.0",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@vitejs/plugin-react-swc": "3.5.0",
    "eslint": "9.9.0",
    "eslint-config-prettier": "10.1.3",
    "eslint-plugin-react": "7.35.0",
    "eslint-plugin-react-hooks": "5.1.0-rc.0",
    "eslint-plugin-react-refresh": "0.4.9",
    "globals": "15.9.0",
    "husky": "9.1.7",
    "lint-staged": "15.5.1",
    "msw": "^2.12.3",
    "npm-run-all": "4.1.5",
    "prettier": "3.5.3",
    "pretty-quick": "4.1.1",
    "vite": "5.4.1"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx}": [
      "eslint --fix"
    ],
    "src/**/*.{ts,tsx,js,jsx,css,json}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=18"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
